{
  "name": "tauri-app",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "kill-port": "node -e \"const{execSync}=require('child_process');try{const r=execSync('netstat -ano | findstr :1420 | findstr LISTENING',{encoding:'utf8'});const pid=r.trim().split(/\\s+/).pop();if(pid&&pid!=='0')execSync('taskkill /F /PID '+pid)}catch{}\"",
    "dev": "npm run kill-port && vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.10.1",
    "@tauri-apps/plugin-opener": "^2",
    "liquid-glass-react": "^1.1.1",
    "marked": "^17.0.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.3",
    "typescript": "~5.6.2",
    "vite": "^6.0.3"
  }
}
